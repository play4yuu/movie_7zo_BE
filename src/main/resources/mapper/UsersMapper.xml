<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.campus.movie.mapper.UsersMapper">
	<select id="loginSelect" resultType="com.campus.movie.vo.UsersVO">
		select userid, username from users where userid=#{param1} and userpwd=#{param2}
	</select>
	<select id="movieloginSelect" resultType="com.campus.movie.vo.UserVO">
		select id, name from user where id=#{param1} and pw=#{param2}
	</select>
</mapper>
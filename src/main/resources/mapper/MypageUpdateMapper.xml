<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.campus.movie.mapper.MypageUpdateMapper">
 <update id="updateUser" parameterType="com.campus.movie.vo.UserDTO">
        UPDATE user
        SET
          pw = #{pw},
          email = #{email},
          name = #{name},
          phone = #{phone},
          nickname = #{nickname}
   		  WHERE id = #{id}
    </update>
</mapper>